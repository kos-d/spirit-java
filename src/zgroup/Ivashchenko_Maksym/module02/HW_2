class ProfShop {
    public boolean isPriceOk(int price){
        return price != 1000;
    }
    public float calculateRegularDiscountPrice(float price){
        return price = price*0.9f;
    }
    public boolean isDiscount50(int price){
        return price == 1000/4;
    }
    public boolean isPriceHappy(int price, int year, int day){
        return price == year * day;
    }

    //Expected output
    public static void main(String[] args) {
        ProfShop price1 = new ProfShop();
        System.out.println(isPriceOk(1500));
        System.out.println(calculateRegularDiscountPrice(1500));
        System.out.println(isDiscount50(256));
        System.out.println(isPriceHappy(23700, 3950, 6));
    }
}